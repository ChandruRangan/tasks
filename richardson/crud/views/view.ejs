    <form action="/search"  method="post">
        <div class="form-group">
            <select class="form-control" name="input">
                <%for(var i=0 ;i<cat.length; i++) { %>
                    <option value="<%=cat[i].category_name%>"><%=cat[i].category_name%></option>
                    <%}%>
                </select>
                <input type="submit" class="btn btn-dark mt-2" placeholder="Search">
            </div> 
        </form>
<head>
    <link
      rel="stylesheet"
      href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css"
      integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T"
      crossorigin="anonymous"
    />
<body>
<table class="table table-striped">
    <thead>
        <tr>
            <th>Product Id</th>
            <th>Product Name</th>
            <th>Category Name <a href="/casc?c=category_name" class="btn btn-primary">Sort</a></th>
            <th>Price</th>
            <th>created_at</th>
            <th>updated_at</th>
        </tr>
    </thead>
    <tbody>
        <%for (var i=0; i<data.length; i++){%>
            <tr>
            <td><%=data[i].products_id%></td>
            <td><%=data[i].product_name%></td>
            <td><%=data[i].category_name%></td>
            <td><%=data[i].price%></td>
            <td><%=data[i].created_at%></td>
            <td><%=data[i].updated_at%></td>
            <td><a href="/update?id=<%=data[i].products_id%>" class="btn btn-primary">Update</a></td>
            <td><a href="/delete?id=<%=data[i].products_id%>" class="btn btn-primary">Delete</a></td>
            </tr>
        <%}%>
    </tbody>

</table>
<a href="/" class="btn btn-primary">back</a>
  
</body>
</head>
</div>