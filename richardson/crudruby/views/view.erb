<!doctype html>
<html lang="en">
  <head>
    <title>Search</title>
    <form action="/pro_search"  method="post">
        <div class="form-group">
            <select class="form-control" name="full_name">
                <%emp.each do |val|%>
                    <option value="<%=val['full_name']%>"><%=val['full_name']%></option>
                <%end%>
                </select>
                <input type="submit" class="btn btn-dark mt-2">
            </div> 
        </form>
        
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">
<table class="table table-striped">
    <thead>
        <tr>
            <th>Employee Id</th>
            <th>Name</th>
            <th>Email</th>
            <th>Phone_number</th>
            <th>Join_date</th>
            <th>Date of Birth</th>
        </tr>
    </thead>
    <tbody>
    <%emp.each do |val|%>
        <tr>
        <td><%=val['employee_id']%></td>
        <td><%=val['full_name']%></td>
        <td><%=val['email']%></td>
        <td><%=val['phone_number']%></td>
        <td><%=val['join_date']%></td>
        <td><%=val['date_of_birth']%></td>
        <td><a href="/update?id=<%=val['employee_id']%>" class="btn btn-outline-success">Update</a></td>
        <td><a href="/delete?id=<%=val['employee_id']%>" class="btn btn-outline-danger">Delete</a></td>
        </tr> 
        <%end%>
    </tbody>

</table>
<a href="/" class="btn btn-primary">Addemployee</a>
<a href="/project" class="btn btn-primary">Addproject</a>
<a href="/project_view" class="btn btn-primary">projects</a>