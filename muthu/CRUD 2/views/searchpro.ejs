<%-include('style.ejs')%>
<div class="container-fluid mt-5">
    <form action="/searchpro" autocomplete="off" method="post">
        <div class="form-group">
            <input type="text" class="form-control" placeholder="Search here..." name="input">
            <input type="submit" class="btn btn-dark mt-2" placeholder="Search">
        </div>
    </form>
<table class="table table-borderd">
    <thead class="thead-dark">
           <tr>
               <th>Project Id</th>
               <th>Project Name</th>
               <th>Project Lead</th>
               <th>Project Start Date</th>
               <th>Project End Date</th>
               <th>Action</th>
           </tr> 
</thead>
  <tbody>
    <%for(var i=0;i<project.length;i++){%>
        <tr>
            <td><%=project[i].project_id%></td>
            <td><%=project[i].projectname%></td>
            <td><%=project[i].projectlead%></td>
            <td><%=project[i].project_start_date%></td>
            <td><%=project[i].project_end_date%></td>
            <td><a href="/update" class="btn btn-success">Update</a></td>
        </tr>
        <%}%>
    </tbody>
</table>
<div class="text-center">
    <a class="btn btn-info" href="/display">Back</a>
</div>
</div>
