<%# stylesheets and design%>
<%=erb :style%>

<body class="w3-content">
<div class="w3-container w3-margin-top w3-animate-top">
<form action="/dash" method="post" class="w3-center w3-padding w3-card-4">
<h2 class="w3-container w3-blue">Login</h2>
    <input type="email" class="w3-input w3-border w3-section" placeholder="Email Address" name="email">
    <input type="password" class="w3-input w3-border w3-section" placeholder="Password" name="password">
    <input type="submit" class="w3-button w3-green w3-round">
</form>
</div>
</body>