<!doctype html>
<html lang="en">
  <head>
    <title>Title</title>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">
  </head>
  <body>
      <form action="/view" method="post"></form>
      <div>
          <table class="table table-bordered">
              <thead>
                  <tr>
                      <th>full_name</th>
                      <th>email_addrerss</th>
                      <th>phone_no</th>
                      <th>joining_date</th>
                      <th>dob</th>
                      <th>emp_id</th>
                  </tr>
              </thead>
              <tbody>
              <%values.each do |s| %>
              <tr>
                 <td><%=s['full_name']%></td>
                 <td><%=s['email_address']%></td>
                 <td><%=s['phone_no']%></td>
                 <td><%=s['joining_date']%></td>
                 <td><%=s['dob']%></td>
                 <td><%=s['emp_id']%></td>
                 <td><a href="/update?id=<%= s['emp_id']%>" class="btn btn-primary">Update</a></td>
                 <td><a href="/delete?id=<%= s['emp_id']%>" class="btn btn-danger">Delete</a></td>
                 </tr> 
                 <%end%>  
                 </tbody>
          </table>
      </div>
  </body>
</html>