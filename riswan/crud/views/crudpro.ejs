<!doctype html>
<html lang="en">
  <head>
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">
  </head>
    <body class="bg-info">
      <div class="container mt-5">
        <form action="/insert" method="post">
          <div class="form-group">
          Product Name <input class="form-control"  type="text" name="pn" placeholder="Product Name">
          </div>
          <div class="form-group">
            Category<select class="form-control"  name="category">
              <option>Enter Category</option>
              <% for(var i = 0; i <data.length; i++){ %>
                <option value="<%= data[i].category_id%>"><%=data[i].category%></option>
                <%}%>
              </select>
            </div>
            <div class="form-group">
             Price <input class="form-control"  type="text" name="price" placeholder="Price">
            </div>
            <div class="container text-center">
        <button class="btn btn-dark" type="submit" name="search">Submit</button>
      </div>
        <a href="/dis" class="text-secondary">display table</a>
      </div>
    </form>
  </div>   
  </body>
</html>